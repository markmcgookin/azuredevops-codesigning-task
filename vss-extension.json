{
    "manifestVersion": 1,
    "id": "codesigningwithdescription",
    "name": "Code Signing With Description",
    "version": "2.1.13",
    "public": true,
    "publisher": "markmcgookin",
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "description": "Task that gives the user the ability to codesign assemblies and applications, including a description.",
    "categories": [
        "Azure Pipelines"
    ],
    "galleryFlags": [],
    "tags": [
        "release",
        "build",
        "codesigning",
        "signing",
        "signtool",
        "sign",
        "cert",
        "certificate"
    ],
    "links": {
        "support": {
            "uri": "https://github.com/markmcgookin/azuredevops-codesigning-task/issues"
        }
    },
    "icons": {
        "default": "images/extension-icon.png"
    },
    "branding": {
        "color": "#028c7e",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/markmcgookin/azuredevops-codesigning-task"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "files": [{
            "path": "task"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "contributions": [{
        "id": "codesigning-task",
        "type": "ms.vss-distributed-task.task",
        "targets": [
            "ms.vss-distributed-task.tasks"
        ],
        "properties": {
            "name": "task"
        }
    }]
}